{
	"name": "questioner",
	"version": "1.0.0",
	"description": "Crowd-source questions for a meetup. ​ Questioner​​ helps the meetup organizer prioritize questions to be answered.",
	"main": "server.js",
	"scripts": {
		"lint": "eslint ./src --fix",
		"start": "nodemon --exec babel-node ./server.js",
		"build": "babel src --out-dir ./dist --source-maps",
		"serve": "node ./dist/server.js",
		"debug": "node --inspect-brk ./dist/server",
		"test": "nyc --reporter=html --reporter=text mocha --require @babel/register --recursive ./server/test/**/*.spec.js --exit",
		"coverage": "nyc report --reporter=text-lcov | coveralls"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dbytecoderc/Questioner.git"
	},
	"author": "DC",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/dbytecoderc/Questioner/issues"
	},
	"homepage": "https://github.com/dbytecoderc/Questioner#readme",
	"dependencies": {
		"body-parser": "^1.18.3",
		"dotenv": "^6.2.0",
		"express": "^4.16.4",
		"moment": "^2.23.0",
		"morgan": "^1.9.1",
		"path": "^0.12.7",
		"uuid": "^3.3.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.2.2",
		"@babel/node": "^7.2.2",
		"@babel/preset-env": "^7.2.3",
		"@babel/register": "^7.0.0",
		"babel-istanbul": "^0.12.2",
		"babel-plugin-istanbul": "^5.1.0",
		"chai": "^4.2.0",
		"chai-http": "^4.2.0",
		"coveralls": "^3.0.2",
		"cross-env": "^5.2.0",
		"eslint": "^5.11.0",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-plugin-import": "^2.14.0",
		"istanbul": "^0.4.5",
		"mocha": "^5.2.0",
		"mocha-lcov-reporter": "^1.3.0",
		"nodemon": "^1.18.9",
		"supertest": "^3.3.0"
	}
}